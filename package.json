{
  "name": "i18n-util",
  "version": "1.0.0",
  "description": "Translation (i18n) file management.",
  "homepage": "https://github.com/JuroOravec/i18n-util#readme",
  "repository": "github:JuroOravec/i18n-util",
  "bugs": "https://github.com/JuroOravec/i18n-util/issues",
  "author": {
    "name": "Juro Oravec",
    "email": "juraj.oravec.josefson@gmail.com"
  },
  "keywords": [
    "vue",
    "i18n",
    "internationalization",
    "localization",
    "vue-i18n",
    "vue-i18n-extract"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "bin": "./bin/index.js",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    },
    "chonky": {
      "node": {
        "version": "1.0.1"
      }
    }
  },
  "snyk": true,
  "husky": {
    "hooks": {
      "pre-commit": "git-authors-cli && run-s lint format:check",
      "prepare-commit-msg": "sh ./scripts/prepare_commit_msg.sh",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "npm run validate"
    }
  },
  "scripts": {
    "build": "./node_modules/typescript/bin/tsc",
    "watch": "./node_modules/typescript/bin/tsc --watch",
    "test": "snyk test && npm run test:cov",
    "test:cov": "run-s test:jest codecov:upload",
    "test:jest": "jest --config jestconfig.json --detectOpenHandles",
    "codecov:upload": "dotenv bash scripts/upload_codecov.sh",
    "format": "prettier --write 'src/**/*.{ts,js}'",
    "format:check": "prettier --check 'src/**/*.{ts,js}'",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:check": "eslint . --ext .js,.jsx,.ts,.tsx",
    "validate": "run-s test:jest lint:check format:check snyk:test",
    "clean": "shx rm -rf node_modules/ dist/",
    "setup": "npm run build",
    "prepare": "npm run snyk:protect",
    "prepublishOnly": "npm run snyk:protect",
    "typedoc:markdown": "typedoc --out ./docs/typedoc --mode file --excludeNotExported --exclude '**/__tests__/**/*' --tsconfig ./tsconfig.json --readme none --plugin typedoc-plugin-markdown ./src/",
    "typedoc:html": "typedoc --out ./docs/typedoc --mode file --excludeNotExported --exclude '**/__tests__/**/*' --tsconfig ./tsconfig.json --readme none --plugin none ./src/",
    "snyk:protect": "snyk protect",
    "snyk:test": "snyk test",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate",
    "labels:sync": "node scripts/sync_labels.js",
    "readme:init": "node ./node_modules/readme-md-generator -p README_template.md"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@commitlint/travis-cli": "^8.3.5",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.0.6",
    "@semantic-release/npm": "^7.0.5",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "@types/jest": "^25.2.2",
    "@types/node": "^14.0.12",
    "@types/rimraf": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "all-contributors-cli": "^6.20.0",
    "codecov": "^3.6.5",
    "commitizen": "^4.0.4",
    "dotenv-cli": "^3.1.0",
    "env-ci": "^5.0.2",
    "eslint": "^6.8.0",
    "git-authors-cli": "^1.0.24",
    "github-label-sync": "^1.6.0",
    "husky": "^4.2.5",
    "inquirer": "^7.1.0",
    "jest": "^25.3.0",
    "js-yaml": "^3.13.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "readme-md-generator": "^1.0.0",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.0.4",
    "semantic-release-changelog-update": "^1.1.8",
    "semantic-release-cli": "^5.2.3",
    "semantic-release-verify-deps": "^1.0.2",
    "shx": "^0.3.2",
    "snyk": "^1.360.0",
    "standard-version": "^8.0.0",
    "ts-jest": "^25.3.1",
    "typedoc": "^0.17.4",
    "typedoc-plugin-markdown": "^2.2.17",
    "typescript": "^3.9.2"
  },
  "dependencies": {
    "@types/debug": "^4.1.5",
    "@types/glob": "^7.1.1",
    "@types/is-glob": "^4.0.1",
    "@types/js-yaml": "^3.12.4",
    "@types/jsdom": "^16.2.3",
    "@types/lodash.chunk": "^4.2.6",
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/lodash.differenceby": "^4.8.6",
    "@types/lodash.flatten": "^4.4.6",
    "@types/lodash.flattendeep": "^4.4.6",
    "@types/lodash.frompairs": "^4.0.6",
    "@types/lodash.get": "^4.4.6",
    "@types/lodash.groupby": "^4.6.6",
    "@types/lodash.intersection": "^4.4.6",
    "@types/lodash.intersectionwith": "^4.4.6",
    "@types/lodash.isobject": "^3.0.6",
    "@types/lodash.merge": "^4.6.6",
    "@types/lodash.pick": "^4.4.6",
    "@types/lodash.unionby": "^4.8.6",
    "@types/lodash.uniq": "^4.5.6",
    "@types/lodash.uniqby": "^4.7.6",
    "@types/lodash.zip": "^4.2.6",
    "@types/micromatch": "^4.0.1",
    "@types/read-pkg-up": "^6.0.0",
    "@types/tmp": "^0.2.0",
    "cli-table3": "^0.6.0",
    "cli-width": "^3.0.0",
    "commander": "^5.1.0",
    "debug": "^4.1.1",
    "generate-schema": "^2.6.0",
    "glob": "^7.1.6",
    "is-glob": "^4.0.1",
    "jsdom": "^16.2.2",
    "lodash.chunk": "^4.2.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.differenceby": "^4.8.0",
    "lodash.flatten": "^4.4.0",
    "lodash.flattendeep": "^4.4.0",
    "lodash.frompairs": "^4.0.1",
    "lodash.get": "^4.4.2",
    "lodash.groupby": "^4.6.0",
    "lodash.intersection": "^4.4.0",
    "lodash.intersectionwith": "^4.4.0",
    "lodash.isobject": "^3.0.2",
    "lodash.merge": "^4.6.2",
    "lodash.pick": "^4.4.0",
    "lodash.unionby": "^4.8.0",
    "lodash.uniq": "^4.5.0",
    "lodash.uniqby": "^4.7.0",
    "lodash.zip": "^4.2.0",
    "micromatch": "^4.0.2",
    "read-pkg-up": "^7.0.1",
    "schema-utils": "^2.6.6",
    "tmp": "^0.2.1",
    "vue-i18n-extract": "^1.1.1"
  }
}
